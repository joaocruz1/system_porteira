
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model pedidos {
  id          Int     @id @default(autoincrement())
  data_pedido DateTime 
  status      String
  total       Decimal
  image       String
}